---
import Column from "../../layouts/Column.astro";
import DeveloperItem from "./DeveloperItem.astro";
import logoAxia from "../../assets/logos/logo_axia_baires_banner.png"
import logoMetrocons from "../../assets/logos/logo_metrocons.png";
import logoMateria from "../../assets/logos/logo_materia_banner.png";
---

<section id="desarrolladores" class="pt-20 pb-20 bg-stone-200">
  <Column>
    <li class="flex flex-wrap md:flex-nowrap gap-8 justify-around">
      <DeveloperItem title="axia baires" img={logoAxia} alt="Logo de Axia Baires"/>
      <DeveloperItem title="metrocons" img={logoMetrocons} alt="Logo de metrocons" />
      <DeveloperItem title="materia arquitectos" img={logoMateria} alt="Logo de metrocons" />
    </li>
  </Column>
</section>

